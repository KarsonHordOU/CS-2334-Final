    private static void testDouble() {
        System.out.println("MyArrayList<Double> addFirst:     " + myAddFirstDouble() + " ns");
        System.out.println("MyArrayList<Double> addMiddle:    " + myAddMiddleDouble() + " ns");
        System.out.println("MyArrayList<Double> addLast:      " + myAddLastDouble() + " ns");
        System.out.println("MyArrayList<Double> seq access:   " + mySequentialDouble() + " ns");
        System.out.println("MyArrayList<Double> random access:" + myRandomDouble() + " ns");
        System.out.println("MyArrayList<Double> removeFirst:  " + myRemoveFirstDouble() + " ns");
        System.out.println("MyArrayList<Double> removeMiddle: " + myRemoveMiddleDouble() + " ns");
        System.out.println("MyArrayList<Double> removeLast:   " + myRemoveLastDouble() + " ns");

        System.out.println();

        System.out.println("Java ArrayList<Double> addFirst:  " + javaAddFirstDouble() + " ns");
        System.out.println("Java ArrayList<Double> addMiddle: " + javaAddMiddleDouble() + " ns");
        System.out.println("Java ArrayList<Double> addLast:   " + javaAddLastDouble() + " ns");
        System.out.println("Java ArrayList<Double> seq access:" + javaSequentialDouble() + " ns");
        System.out.println("Java ArrayList<Double> random:    " + javaRandomDouble() + " ns");
        System.out.println("Java ArrayList<Double> removeFirst: " + javaRemoveFirstDouble() + " ns");
        System.out.println("Java ArrayList<Double> removeMiddle:" + javaRemoveMiddleDouble() + " ns");
        System.out.println("Java ArrayList<Double> removeLast:  " + javaRemoveLastDouble() + " ns");
    }

    private static long myAddFirstDouble() {
        MyArrayList<Double> list = new MyArrayList<>();
        long start = System.nanoTime();
        list.addFirst(1.0);
        return System.nanoTime() - start;
    }

    private static long myAddMiddleDouble() {
        MyArrayList<Double> list = new MyArrayList<>();
        for (int i = 0; i < SIZE; i++) {
            list.add((double) i);
        }
        long start = System.nanoTime();
        list.addMiddle(5000.0);
        return System.nanoTime() - start;
    }

    private static long myAddLastDouble() {
        MyArrayList<Double> list = new MyArrayList<>();
        long start = System.nanoTime();
        for (int i = 0; i < SIZE; i++) {
            list.addLast((double) i);
        }
        return System.nanoTime() - start;
    }

    private static long mySequentialDouble() {
        MyArrayList<Double> list = new MyArrayList<>();
        for (int i = 0; i < SIZE; i++) {
            list.add((double) i);
        }
        long start = System.nanoTime();
        for (int i = 0; i < SIZE; i++) {
            list.get(i);
        }
        return System.nanoTime() - start;
    }

